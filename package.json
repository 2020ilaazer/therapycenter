  "repository": {
    "type": "git",
    "url": "git+https://github.com/mohokh67/therapycenter.git"
  },
  "keywords": [],
  "author": "MoHo Khaleqi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mohokh67/therapycenter/issues"
  },
  "homepage": "https://github.com/mohokh67/therapycenter#readme",
  "dependencies": {
    "@zeit/next-sass": "^1.0.1",
    "bulma": "^0.7.2",
    "express": "^4.16.4",
    "google-maps-react": "^2.0.2",
    "moment": "^2.23.0",
    "next": "^7.0.2",
    "node-sass": "^4.11.0",
    "nprogress": "^0.2.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
